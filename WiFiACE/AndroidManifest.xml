<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.marcus905.wifi.ace" android:installLocation="auto" android:versionCode="20120115" android:versionName="0.11">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="false">
        <activity android:name=".WiFiACEList"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity android:name="WiFiACESettings" 
      android:label="WiFi Advanced Config Editor Settings">
    </activity>
</application>
    <uses-sdk android:minSdkVersion="4" />

<uses-feature android:name="android.hardware.touchscreen" android:required="false"/>

<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>

<uses-permission android:name="android.permission.CHANGE_WIFI_STATE"></uses-permission>
</manifest> 